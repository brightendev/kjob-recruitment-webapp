    <script language="JavaScript">
        // NumberOnly
        function chkNumber(ele) {
            var vchar = String.fromCharCode(event.keyCode);
            if ((vchar < '0' || vchar > '9')) return false;
            ele.onKeyPress = vchar;
        }
        // EngOnly
        function chkEng(ele) {
            var vchar = String.fromCharCode(event.keyCode);
            if ((vchar < 'A' || vchar > 'Z') && (vchar < 'a' || vchar > 'z')) return false;
            ele.onKeyPress = vchar;
        }
        // ThaiOnly
        function chkThai(ele) {
            var vchar = String.fromCharCode(event.keyCode);
            if ((vchar < 'ก' || vchar > 'ฮ') && (vchar < 'ะ' || vchar > 'ไ') && (vchar < '่' || vchar > '๋')) return false;
            ele.onKeyPress = vchar;
        }
    </script>


    <!-- ========== ADMINBSB ======== -->
